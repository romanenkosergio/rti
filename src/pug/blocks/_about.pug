                    svg(xmlns='http://www.w3.org/2000/svg' viewbox='0 0 12.81 12.81')
                        defs
                            style.
                                .cls-1 {
                                    fill: #626366;
                                }

                                .cls-2 {
                                    fill: none;
                                    stroke: #fffff3;
                                    stroke-miterlimit: 10;
                                }
                        title &Rcy;&iecy;&scy;&ucy;&rcy;&scy; 10
                        g(id='Слой_2' data-name='Слой 2')
